{
  "name": "@identity-connect/dapp-sdk",
  "version": "0.16.1",
  "private": false,
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./src/": "./src/"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "uuid": "^9.0.1",
    "@aptos-connect/web-transport": "0.6.1",
    "@aptos-connect/wallet-api": "0.6.0",
    "@identity-connect/api": "0.8.0",
    "@identity-connect/wallet-api": "0.2.0",
    "@identity-connect/crypto": "0.3.0"
  },
  "peerDependencies": {
    "@aptos-labs/ts-sdk": "^1.33.1 || ^2.0.0 || ^3.0.0 || ^4.0.0 || ^5.0.0",
    "@aptos-labs/wallet-standard": "^0.5.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^22.18.1",
    "@types/uuid": "^9.0.1",
    "axios-mock-adapter": "1.21.4",
    "jest": "^29.1.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tsup": "^7.1.0",
    "typescript": "5.4.3",
    "@petra/tsconfig": "0.0.0"
  },
  "files": [
    "dist",
    "src",
    "!src/**.test.ts",
    "!src/**/__tests__"
  ],
  "scripts": {
    "build:bundle": "tsup src/index.ts --tsconfig tsconfig.build.json --format cjs,esm --sourcemap",
    "build:declarations": "tsc --project tsconfig.build.json --emitDeclarationOnly --declaration --declarationMap",
    "build": "pnpm build:bundle && pnpm build:declarations",
    "lint": "biome check",
    "lint:fix": "biome check --write --unsafe",
    "lint:tsc": "tsgo --noEmit -p ./tsconfig.json",
    "test": "jest"
  }
}