{
  "name": "@shelby-protocol/reed-solomon",
  "version": "0.0.1",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.11.17",
    "esbuild": "=0.25.6",
    "rimraf": "^6",
    "tsup": "^8.4.0"
  },
  "scripts": {
    "lint": "biome check .",
    "fmt": "biome check . --write",
    "build": "rimraf dist; tsc; tsup",
    "start": "node dist/index.js"
  }
}